	"author": "",
	"license": "GPL-3.0",
	"dependencies": {
		"@types/node": "^20.11.20",
		"@types/vcf": "^2.0.7",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.23.0",
		"obsidian": "latest",
		"tsdav": "^2.0.9",
		"tslib": "^2.6.2",
		"typescript": "^5.5.4",
		"vcf": "^2.1.2",
		"xml-js": "^1.6.11"
	},
	"devDependencies": {
		"@jest/globals": "^29.7.0",
